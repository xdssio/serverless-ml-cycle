{
  "dev": {
    "app_function": "app.app",
    "aws_region": "eu-west-1",
    "profile_name": "xdss",
    "project_name": "serverlessml",
    "runtime": "python3.6",
    "slim_handler": true,
    "keep_warm": false,
    "apigateway_enabled": true,
    "use_precompiled_packages": true,
    "s3_bucket": "xdss-serverless-ml",
    "exclude": [
      "*.gz",
      "*.rar",
      "tests",
      "datasets",
      "notebooks",
      "docs",
      "ve",
      "tmp"
