{
  "dev": {
    "app_function": "app.app",
    "aws_region": "eu-west-1",
    "profile_name": "xdss",
    "project_name": "serverlessml",
    "runtime": "python3.6",
    "s3_bucket": "xdss-serverless-ml",
    "models_bucket": "serverless-ml-models",
    "data_bucket": "xdss-public-datasets",
    "data_key": "kaggle/titanic_data.csv",
    "model_name": "titanic-model",
    "tmp_folder": "/tmp",
    "model_key_name": "model.pkl",
    "model_target": "Survived",
    "model_features": [
      "Fare",
      "Age",
      "Pclass",
      "SibSp",
      "Embarked",
      "Sex"
    ],
    "aws_environment_variables": {
      "ENVIRONMENT": "dev",
      "STAGE": "dev",
      "VERSION": "1"
    },
    "environment_variables": {
      "ENVIRONMENT": "dev",
      "STAGE": "dev",
      "VERSION": "1"
    },
    "exclude": [
      "*.gz",
      "*.rar",
      "tests",
      "datasets",
      "notebooks",
      "docs",
      "ve",
      "tmp"
    ]
  },
  "slim_handler": true,
  "keep_warm": false,
  "apigateway_enabled": true,
  "use_precompiled_packages": true
}